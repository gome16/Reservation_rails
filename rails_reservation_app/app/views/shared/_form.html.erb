<%= form_with model: @form, local: true do |f| %>

  <div class="field">
    <%= f.label :date, "日付" %>
    <%= f.date_select :date, use_month_numbers: true, start_year: Time.now.year - 20, end_year: Time.now.year %>

    <%= render "layouts/error_messages", model: @form, attribute: :date %>
  </div>
