<div class="container-fuluid"> 
  <div class="container" >
    <div class="mt-5">
      <div class="row">
          <p class="col-md-2">開始日</p>
        <div class="col-md-5">
          <%= @reservation.check_in%>
        </div>
      </div>
    </div>
   <div class="mt-5">
      <div class="row">
        <div class="col-md-3">
          <p>終了日</p>
        </div>
        <div class="col-md-5">
          <%= @reservation.check_out%>
        </div>
      </div>
    </div>
    <div class="mt-5">
      <div class="row">
        <div class="col-md-3">
          <p>人数</p>
        </div>
        <div class="col-md-5">
          <%= @reservation.number%>
        </div>
      </div>
    </div>
  <div class="mt-5">
    <%= link_to "予約一覧",reservations_path()%>
  </div>
</div>

    